{
  "expo": {
    "name": "RentalEase Technician",
    "slug": "RentalEase_Technician",
    "version": "1.0.0",
    "owner": "juhan.rokoautomations",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rentalease.technician",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UILaunchStoryboardName": "SplashScreen",
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.rentalease.technician"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow RentalEase to access your photos to attach inspection images.",
          "cameraPermission": "Allow RentalEase to access your camera to take inspection photos."
        }
      ],
      "expo-font",
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow RentalEase to access your calendar to sync job schedules and send reminders.",
          "remindersPermission": "Allow RentalEase to set reminders for upcoming jobs."
        }
      ]
    ],
    "scheme": "rentalease",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f04bd6f3-3938-48b7-9807-a95724efde88"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f04bd6f3-3938-48b7-9807-a95724efde88"
    }
  }
}
