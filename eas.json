{
  "cli": {
    "version": "16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development-device": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://server.rentalease.com.au"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://server.rentalease.com.au"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "companyName": "RentalEase Pty Ltd",
        "language": "en-AU",
        "sku": "rentalease-technician-ios"
      }
    }
  }
}
